<div class="form-field">
  <%= simple_form_for @farmer do |f| %>
    <%= f.input :farm_name, class: "form-control" %>
    <%= f.input :first_name, class: "form-control" %>
    <%= f.input :last_name, class: "form-control" %>
    <%= f.input :origin, class: "form-control" %>
    <%= f.input :region, class: "form-control" %>
